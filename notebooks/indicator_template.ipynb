    "## 10. Backtesting Results\n",
    "\n",
    "Let's backtest our strategies and analyze the results."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def backtest_strategy(data, initial_capital=10000):\n",
    "    \"\"\"\n",
    "    Simple backtesting function\n",
    "    \"\"\"\n",
    "    df = data.copy()\n",
    "    df['position'] = df['signal'].shift(1)\n",
    "    df['returns'] = df['close'].pct_change()\n",
    "    df['strategy_returns'] = df['position'] * df['returns']\n",
    "    df['cumulative_returns'] = (1 + df['strategy_returns']).cumprod()\n",
    "    df['portfolio_value'] = initial_capital * df['cumulative_returns']\n",
    "    \n",
    "    return df\n",
    "\n",
    "# Backtest strategy\n",
    "df_backtest = backtest_strategy(df_signals)\n",
    "\n",
    "# Calculate performance metrics\n",
    "total_return = df_backtest['portfolio_value'].iloc[-1] / initial_capital - 1\n",
    "sharpe_ratio = df_backtest['strategy_returns'].mean() / df_backtest['strategy_returns'].std() * np.sqrt(252)\n",
    "max_drawdown = (df_backtest['portfolio_value'] / df_backtest['portfolio_value'].cummax() - 1).min()\n",
    "\n",
    "print(f\"Total Return: {total_return:.2%}\")\n",
    "print(f\"Sharpe Ratio: {sharpe_ratio:.2f}\")\n",
    "print(f\"Maximum Drawdown: {max_drawdown:.2%}\")\n",
    "\n",
    "# Plot equity curve\n",
    "plt.figure(figsize=(12, 6))\n",
    "plt.plot(df_backtest.index, df_backtest['portfolio_value'], label='Strategy', color='blue')\n",
    "plt.axhline(y=initial_capital, color='red', linestyle='--', label='Initial Capital')\n",
    "plt.title('Equity Curve')\n",
    "plt.xlabel('Date')\n",
    "plt.ylabel('Portfolio Value ($)')\n",
    "plt.legend()\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.show()"
   ]
